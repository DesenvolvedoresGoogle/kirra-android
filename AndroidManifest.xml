<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.kirra_android_gdg"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="19"
        android:targetSdkVersion="19" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.kirra_android_gdg.EntityListActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.kirra_android_gdg.EntityDetailActivity"
            android:label="@string/title_entity_detail"
            android:parentActivityName="com.example.kirra_android_gdg.EntityListActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.kirra_android_gdg.EntityListActivity" />
        </activity>
        <activity
            android:name="com.example.kirra_android_gdg.InstanceDetailActivity"
            android:label="@string/title_activity_instance_detail" >
        </activity>
    </application>

</manifest>
